  },
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/preset-env": "7.11.5",
    "@babel/preset-react": "7.10.4",
    "@relative-ci/agent": "1.3.0",
    "babel-loader": "8.1.0",
    "cross-env": "7.0.2",
    "css-loader": "4.3.0",
    "file-loader": "6.1.0",
    "file-url": "3.0.0",
    "html-webpack-plugin": "4.5.0",
    "husky": "4.3.0",
    "lint-staged": "10.4.0",
